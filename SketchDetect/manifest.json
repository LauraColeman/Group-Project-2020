{
    "name": "SketchDetect",
    "version": "1.1",
    "description": "Chrome extension designed to notify users of potentially hazardous websites",
    "manifest_version": 2,

    "permissions": [
        "tabs"
    ],
    "background":{
        "run_at": "document_start",
        "scripts" : ["background.js"],
        "persistent" : false
    }
  }